{
    "manifest_version": 3,
    "name": "Tab Manager for Chrome Dev Docs",
    "version": "1.0",
    "icons": {
      "16": "images/16.png",
      "48": "images/48.png",
      "128": "images/128.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "https://developer.chrome.com/*"
    ],
    "permissions": [
      "tabGroups"
    ]
  }